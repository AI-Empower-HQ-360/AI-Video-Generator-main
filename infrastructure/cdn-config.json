{
  "cdn_config": {
    "global_settings": {
      "cache_policy": "optimized",
      "compression": true,
      "http2_support": true,
      "ipv6_support": true,
      "security_headers": true
    },
    "cache_behaviors": {
      "static_assets": {
        "path_pattern": "/static/*",
        "cache_ttl": 31536000,
        "headers": ["Accept-Encoding", "CloudFront-Viewer-Country"],
        "query_strings": false,
        "cookies": "none"
      },
      "api_responses": {
        "path_pattern": "/api/*",
        "cache_ttl": 300,
        "headers": ["Authorization", "Content-Type", "Accept-Language"],
        "query_strings": true,
        "cookies": "whitelist",
        "allowed_cookies": ["session_id", "user_region", "user_language"]
      },
      "html_pages": {
        "path_pattern": "/*",
        "cache_ttl": 3600,
        "headers": ["Accept-Language", "CloudFront-Viewer-Country"],
        "query_strings": true,
        "cookies": "whitelist",
        "allowed_cookies": ["language", "currency", "region"]
      },
      "media_content": {
        "path_pattern": "/media/*",
        "cache_ttl": 604800,
        "headers": ["Accept-Encoding"],
        "query_strings": false,
        "cookies": "none"
      }
    },
    "geo_restrictions": {
      "type": "none",
      "countries": []
    },
    "ssl_settings": {
      "minimum_protocol_version": "TLSv1.2",
      "ssl_support_method": "sni-only",
      "certificate_source": "acm"
    },
    "regional_edge_caches": [
      {
        "region": "us-east-1",
        "pop_locations": ["IAD", "DCA", "BWI"],
        "price_class": "PriceClass_All"
      },
      {
        "region": "us-west-2", 
        "pop_locations": ["SEA", "PDX", "SFO"],
        "price_class": "PriceClass_All"
      },
      {
        "region": "eu-west-1",
        "pop_locations": ["DUB", "LHR", "MAN"],
        "price_class": "PriceClass_All"
      },
      {
        "region": "eu-central-1",
        "pop_locations": ["FRA", "MUC", "BER"],
        "price_class": "PriceClass_All"
      },
      {
        "region": "ap-southeast-1",
        "pop_locations": ["SIN", "KUL", "BKK"],
        "price_class": "PriceClass_100"
      },
      {
        "region": "ap-northeast-1",
        "pop_locations": ["NRT", "KIX", "ICN"],
        "price_class": "PriceClass_100"
      },
      {
        "region": "me-south-1",
        "pop_locations": ["BAH", "DXB", "DOH"],
        "price_class": "PriceClass_100"
      }
    ],
    "error_pages": {
      "404": {
        "response_code": 404,
        "response_page_path": "/404.html",
        "cache_ttl": 300
      },
      "500": {
        "response_code": 500,
        "response_page_path": "/500.html",
        "cache_ttl": 0
      },
      "503": {
        "response_code": 503,
        "response_page_path": "/maintenance.html",
        "cache_ttl": 60
      }
    },
    "compression_settings": {
      "gzip_types": [
        "text/html",
        "text/css",
        "text/javascript",
        "application/javascript",
        "application/json",
        "text/xml",
        "application/xml",
        "text/plain"
      ],
      "brotli_support": true
    },
    "security_headers": {
      "strict_transport_security": "max-age=31536000; includeSubDomains; preload",
      "content_type_options": "nosniff",
      "frame_options": "DENY",
      "xss_protection": "1; mode=block",
      "referrer_policy": "strict-origin-when-cross-origin",
      "content_security_policy": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self' https:; connect-src 'self' https:; media-src 'self' https:; object-src 'none'; frame-ancestors 'none';"
    },
    "logging": {
      "access_logs": true,
      "real_time_logs": true,
      "log_bucket": "ai-heart-platform-cdn-logs",
      "log_prefix": "access-logs/",
      "log_fields": [
        "timestamp",
        "request_ip",
        "request_method",
        "request_uri",
        "status_code",
        "response_size",
        "referrer",
        "user_agent",
        "edge_location",
        "response_time"
      ]
    }
  },
  "regional_configurations": {
    "us-east-1": {
      "origin_domain": "us-east-1.ai-heart-platform.com",
      "aliases": ["api-us.ai-heart-platform.com", "cdn-us.ai-heart-platform.com"],
      "price_class": "PriceClass_All"
    },
    "us-west-2": {
      "origin_domain": "us-west-2.ai-heart-platform.com", 
      "aliases": ["api-usw.ai-heart-platform.com", "cdn-usw.ai-heart-platform.com"],
      "price_class": "PriceClass_All"
    },
    "eu-west-1": {
      "origin_domain": "eu-west-1.ai-heart-platform.com",
      "aliases": ["api-eu.ai-heart-platform.com", "cdn-eu.ai-heart-platform.com"],
      "price_class": "PriceClass_All"
    },
    "eu-central-1": {
      "origin_domain": "eu-central-1.ai-heart-platform.com",
      "aliases": ["api-euc.ai-heart-platform.com", "cdn-euc.ai-heart-platform.com"],
      "price_class": "PriceClass_All"
    },
    "ap-southeast-1": {
      "origin_domain": "ap-southeast-1.ai-heart-platform.com",
      "aliases": ["api-asia.ai-heart-platform.com", "cdn-asia.ai-heart-platform.com"],
      "price_class": "PriceClass_100"
    },
    "ap-northeast-1": {
      "origin_domain": "ap-northeast-1.ai-heart-platform.com",
      "aliases": ["api-japan.ai-heart-platform.com", "cdn-japan.ai-heart-platform.com"],
      "price_class": "PriceClass_100"
    },
    "me-south-1": {
      "origin_domain": "me-south-1.ai-heart-platform.com",
      "aliases": ["api-me.ai-heart-platform.com", "cdn-me.ai-heart-platform.com"],
      "price_class": "PriceClass_100"
    }
  }
}